{
    "head": {
        "type": "header",
        "description": "Most settings are not automatically saved when modified.\nPress any of the \"Click here to save settings\" buttons for the settings to take effect."
    },
    "button0": {
        "type": "button",
        "description": "Click here to save settings",
        "callback": "_hardRefreshAll"
    },
    "head0": {
        "type": "header",
        "description": "Applet settings"
    },
    "pref_overlay_key": {
        "type": "keybinding",
        "indent": true,
        "description": "Keyboard shortcut to open and close the menu",
        "default": "Super_L::Super_R"
    },
    "pref_use_a_custom_icon_for_applet": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Use a custom icon",
        "tooltip": "Unchecking this allows the theme to set the icon"
    },
    "pref_custom_icon_for_applet": {
        "type": "iconfilechooser",
        "indent": true,
        "default": "custom-start-here-linux-mint-symbolic",
        "description": "Icon",
        "tooltip": "Select an icon to show in the panel.",
        "dependency": "pref_use_a_custom_icon_for_applet"
    },
    "pref_custom_label_for_applet": {
        "type": "entry",
        "indent": true,
        "default": "Menu",
        "description": "Custom label",
        "tooltip": "Enter custom text to show in the panel."
    },
    "pref_open_menu_on_hover": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Open the menu on mouse over",
        "tooltip": "Enable opening the menu when the mouse enters the applet."
    },
    "pref_menu_hover_delay": {
        "type": "spinbutton",
        "indent": true,
        "default": 0,
        "min": 0,
        "max": 1000,
        "step": 50,
        "dependency": "pref_open_menu_on_hover",
        "units": "milliseconds",
        "description": "Menu hover delay:",
        "tooltip": "Delay before the menu is opened on entering the applet."
    },
    "sep1": {
        "type": "separator"
    },
    "head1": {
        "type": "header",
        "description": "Menu settings"
    },
    "pref_enable_autoscroll": {
        "type": "checkbox",
        "indent": true,
        "default": true,
        "description": "Enable auto-scrolling in application list",
        "tooltip": "Choose whether or not to enable smooth auto-scrolling in the application list."
    },
    "pref_animate_menu": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Animate open/close menu",
        "tooltip": ""
    },
    "pref_use_alternate_vector_box": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Use alternate category selection method (EXPERIMENTAL)",
        "tooltip": "With this option enabled, an alternate method to select categories will be used. This method can improve the performance of the menu and is extracted from the applet called Configurable Menu by lestcape."
    },
    "pref_disable_new_apps_highlighting": {
        "type": "checkbox",
        "indent": true,
        "default": true,
        "description": "Disable newly installed applications highlighting",
        "tooltip": ""
    },
    "pref_use_hover_feedback": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Use hover feedback",
        "tooltip": "If enabled, and the user picture is shown, every time an application/favorite/place/recent file is hovered or selected with keyboard navigation, its icon will be displayed where the user picture is located."
    },
    "pref_cat_select_on_hover": {
        "type": "checkbox",
        "indent": true,
        "default": true,
        "description": "Select categories on hover (EXPERIMENTAL)",
        "tooltip": "If disabled, categories can be selected by clicking on them instead of just hovering."
    },
    "pref_cat_hover_delay": {
        "type": "spinbutton",
        "indent": true,
        "default": 0,
        "min": 0,
        "max": 1000,
        "step": 50,
        "units": "milliseconds",
        "description": "Category selection delay:",
        "tooltip": "Delay between switching categories."
    },
    "pref_max_recent_files": {
        "type": "spinbutton",
        "indent": true,
        "default": 20,
        "min": 5,
        "max": 100,
        "step": 1,
        "units": "documents",
        "description": "Max recent documents:",
        "tooltip": ""
    },
    "pref_appinfo_display_method": {
        "type": "combobox",
        "indent": true,
        "default": "infobox",
        "description": "Display method for information about applications:",
        "options": {
            "None": "none",
            "Tooltips": "tooltip",
            "Info box": "infobox"
        }
    },
