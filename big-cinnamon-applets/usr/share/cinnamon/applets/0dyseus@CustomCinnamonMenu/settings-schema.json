    "pref_show_recents_button": {
        "type": "checkbox",
        "indent": true,
        "default": true,
        "description": "Show \"Recent documents\" category",
        "tooltip": "For people who want the \"Recent documents\" category hidden without disabling recent documents globally."
    },
    "pref_remember_recently_used_apps": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Show \"Recent Applications\" category",
        "tooltip": "From the moment this option is enabled, every application launched from the menu will be stored and displayed in the \"Recent Applications\" category.\nEvery time this option is disabled, the list of recently used applications will be cleared."
    },
    "pref_hide_applications_list_scrollbar": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Hide applications list scrollbar",
        "tooltip": ""
    },
    "pref_hide_allapps_category": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Hide \"All Applications\" category",
        "tooltip": ""
    },
    "pref_display_favorites_as_category_menu": {
        "type": "checkbox",
        "indent": true,
        "dependency": "pref_hide_allapps_category",
        "default": false,
        "description": "Display \"Favorites\" as a category menu",
        "tooltip": ""
    },
    "pref_swap_categories_box": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Swap categories box",
        "tooltip": "By default, the categories box is to the left of the applications list. With this option enabled, the categories box will be placed to the right of the applications list."
    },
    "pref_apps_info_box_alignment_to_the_left": {
        "type": "checkbox",
        "indent": true,
        "default": false,
        "description": "Align application info box text to the left",
        "tooltip": ""
    },
    "pref_user_picture_placement": {
        "type": "combobox",
        "indent": true,
        "default": 0,
        "description": "User picture placement",
        "options": {
            "Do not show": 0,
            "Left of applications info box": 1,
            "Right of applications info box": 2
        },
        "tooltip": "This option allows to display the user picture on the menu. Clicking on the picture will open \"Account details\". For the image to be shown, the applications info box also has to be displayed."
    },
    "pref_custom_commands_box_placement": {
        "type": "combobox",
        "indent": true,
